{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "postUpdateOptions": [
    "gomodTidy",
    "gomodNoMassage"
  ],
  "ignorePaths": [],
  "packageRules": [
    {
      "matchPackagePatterns": [
        "^github.com/aws/aws-sdk-go-v2"
      ],
      "groupName": "github.com/aws/aws-sdk-go-v2"
    }
  ]
}
